{
  "name": "anton-orchestration-engine",
  "version": "1.0.0",
  "description": "Orchestration engine for Anton v2 visual flow execution",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.ts' --forceExit",
    "test:integration:db": "npm run db:seed && npm run test:integration",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:auth": "tsx prisma/seed-auth.ts",
    "db:reset": "prisma migrate reset --force && npm run db:seed"
  },
  "keywords": [
    "orchestration",
    "claude",
    "automation",
    "workflow"
  ],
  "author": "Anton Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@sentry/node": "^7.91.0",
    "@sentry/profiling-node": "^1.3.2",
    "@trpc/server": "^10.45.0",
    "@xterm/headless": "^5.5.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.11.5",
    "bullmq": "^4.12.7",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.4",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.0",
    "socket.io": "^4.5.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-sentry-log": "^1.0.26",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prisma": "^6.14.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.1.1",
    "typescript": "^5.2.2"
  }
}
